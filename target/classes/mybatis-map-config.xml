			type="com.study.luna.user.comment.vo.EventReplyVO" />
		<typeAlias alias="nReply"
			type="com.study.luna.user.comment.vo.NoticeReplyVO" />



		<!-- Manager,Admin -->
		<typeAlias alias="report"
			type="com.study.luna.pub.command.ReportDTO" />
		<typeAlias alias="reportReply"
			type="com.study.luna.pub.command.ReportReplyDTO" />

